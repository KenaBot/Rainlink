{
  "name": "rainlink",
  "version": "1.0.7",
  "description": "Another lavalink wrapper but focus on stability and rich features",
  "repository": {
    "type": "git",
    "url": "https://github.com/RainyXeon/Rainlink.git"
  },
  "scripts": {
    "build": "npx tsup --config tsup-config.json",
    "prettier": "npx prettier -w ./src",
    "build:doc": "typedoc --options ./typedoc.json --sort kind",
    "build:full": "npm run prettier && npm run build:eslint && npm run build && npm run build:doc",
    "build:eslint": "npx eslint ./src --fix"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "bot",
    "music",
    "lavalink.js",
    "lavalink-api",
    "discord.js",
    "eris.js",
    "oceanic.js",
    "lavalink",
    "wrapper",
    "lavalink-wrapper",
    "rainlink"
  ],
  "author": "RainyXeon",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "discord.js": "^14.14.1",
    "dokdo": "^1.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  }
}
